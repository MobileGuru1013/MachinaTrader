{
  "name": "machinatrader",
  "version": "1.0.0",
  "description": "MachinaTrader assets builder",
  "homepage": "https://github.com/LORDofDOOM/MachinaTrader",
  "license": "MIT",
  "author": {
    "name": "Thiemo Borger",
    "url": "https://github.com/LORDofDOOM"
  },
  "main": "wwwroot/index.html",
  "scripts": {
    "build": "node_modules/.bin/npm-run-all css js build-vendors",
    "build-html": "node_modules/.bin/npm-run-all css js build-clean build-copy build-vendors",
    "build-clean": "node node_modules/rimraf/rimraf dist",
    "build-copy": "copyfiles -a -e \"wwwroot/scss/**/*\" -u 1 \"wwwroot/**/*\" dist/",
    "build-vendors": "node build/vendors.js",
    "css": "node_modules/.bin/npm-run-all css-compile* --sequential css-prefix css-minify*",
    "css-compile": "node node_modules/node-sass/bin/node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 wwwroot/scss/style.scss wwwroot/css/style.css",
    "css-compile-dark": "node node_modules/node-sass/bin/node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 wwwroot/scss/styledark.scss wwwroot/css/styledark.css",
    "_css-compile-vendors": "node build/vendors-sass.js",
    "css-lint": "stylelint --config build/.stylelintrc --syntax scss \"wwwroot/scss/**/*.scss\"",
    "css-minify": "node node_modules/clean-css-cli/bin/cleancss --level 1 --source-map --source-map-inline-sources --output wwwroot/css/style.min.css wwwroot/css/style.css",
    "css-minify-dark": "node node_modules/clean-css-cli/bin/cleancss --level 1 --source-map --source-map-inline-sources --output wwwroot/css/styledark.min.css wwwroot/css/styledark.css",
    "css-prefix": "node node_modules/postcss-cli/bin/postcss --config build/postcss.config.js --replace \"wwwroot/css/*.css\" \"!wwwroot/css/*.min.css\"",
    "js": "node_modules/.bin/npm-run-all js-compile* js-minify*",
    "js-compile": "node node_modules/cross-env/dist/bin/cross-env PLUGINS=true node_modules/@babel/cli/bin/babel.js wwwroot/js/src/ --out-dir wwwroot/js/dist/ --source-maps",
    "js-minify": "node node_modules/uglify-js/bin/uglifyjs --compress typeofs=false --source-map --mangle --comments \"/^!!/\" wwwroot/js/dist/*.js --output wwwroot/js/main.js",
    "js-lint": "eslint wwwroot/js/src",
    "localhost": "browser-sync start --server \"./wwwroot\" --serveStatic \"./\" --files \"!./wwwroot/js/src/**/*, ./wwwroot/**/*, !./wwwroot/scss/**/*\"",
    "localhost-dist": "browser-sync start --server \"./dist\"",
    "release-version": "node build/change-version.js",
    "serve": "node_modules/.bin/npm-run-all --parallel localhost watch-css watch-js",
    "watch-css": "nodemon --ignore dist/ -e scss -x \"npm run css\"",
    "watch-js": "nodemon --watch wwwroot/js/src/ -x \"npm run js\""
  },
  "dependencies": {
    "@aspnet/signalr": "^1.0.0",
    "@coreui/coreui-plugin-chartjs-custom-tooltips": "1.2.0",
    "@machinacore/machinacoreui": "git+https://github.com/MachinaCore/MachinaCoreUI.git#master",
    "@coreui/icons": "^0.3.0",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@mdi/font": "^2.4.85",
    "axios": "^0.18.0",
    "bootbox": "git+https://github.com/makeusabrew/bootbox.git#v5.x",
    "bootstrap": "4.1.1",
    "bootstrap-touchspin": "^4.2.5",
    "bootstrap-daterangepicker": "^3.0.0",
    "bootstrap4-datetimepicker": "git+https://github.com/LORDofDOOM/bootstrap4-datetimepicker.git",
    "chart.js": "^2.7.2",
    "datatables.net": "^1.10.16",
    "datatables.net-bs4": "^1.10.16",
    "datatables.net-responsive-bs4": "^2.2.1",
    "flag-icon-css": "3.0.0",
    "fullcalendar": "^3.9.0",
    "ion-rangeslider": "^2.2.0",
    "jquery": "3.3.1",
    "jquery-ui-dist": "^1.12.1",
    "jquery-validation": "^1.17.0",
    "jquery-validation-unobtrusive": "^3.2.10",
    "jquery.maskedinput": "^1.4.1",
    "ladda": "^1.0.6",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "pace-progress": "1.0.2",
    "perfect-scrollbar": "^1.4.0",
    "popper.js": "1.14.3",
    "qrcodejs2": "0.0.2",
    "qtip2": "^3.0.3",
    "quill": "^1.3.6",
    "select2": "^4.0.6-rc.1",
    "simple-line-icons": "2.4.1",
    "spinkit": "^1.2.5",
    "toastr": "^2.1.4",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "autoprefixer": "^8.6.2",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-es2015-modules-strip": "^0.1.1",
    "browser-sync": "2.24.4",
    "chalk": "^2.4.1",
    "clean-css-cli": "^4.1.11",
    "copyfiles": "^2.0.0",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-plugin-compat": "^2.4.0",
    "foreach-cli": "^1.8.0",
    "globule": "^1.2.1",
    "node-glob": "^1.2.0",
    "node-sass": "^4.9.2",
    "nodemon": "^1.17.4",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^5.0.0",
    "rimraf": "^2.6.2",
    "shelljs": "^0.8.1",
    "stylelint": "^9.2.1",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^3.1.0",
    "uglify-js": "^3.4.0"
  },
  "engines": {
    "node": ">=6"
  },
  "browserslist": [
    "last 1 major version",
    ">= 1%",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ]
}
